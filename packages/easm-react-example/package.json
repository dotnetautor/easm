  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotnetautor/easm.git"
  },
  "scripts": {
    "cleanup": "shx rm -rf ./dist/*",
    "prebuild": "npm run cleanup",
    "build": "webpack --mode production --progress",
    "start": "webpack-dev-server --progress --inline --progress --env debug --content-base dist/ --host 0.0.0.0 --port 3100",
    "postbuild": "node ../../scripts/copy",
    "pack": "npm pack ./dist",
    "postpack": "mkdir -p ../../dist && mv *.tgz ../../dist",
    "docs": "typedoc -p . --theme minimal --target 'es6' --excludeNotExported --excludePrivate --ignoreCompilerErrors --exclude \"**/src/**/__tests__/*.*\" --out docs src/"
  },
  "sideEffects": false,
  "dependencies": {
    "@easm/core": "~0.4",
    "@easm/react": "~0.4",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "core-js": "^3",
    "react": "^17",
    "react-dom": "^17",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "css-loader": "5.1.3",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.3.0",
    "postcss-loader": "5.2.0",
    "raw-loader": "4.0.2",
    "rollup": "3.29.5",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-uglify": "6.0.4",
    "shx": "0.3.3",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.18",
    "tslib": "2.1.0",
    "typedoc": "0.20.32",
    "typescript": "4.2.3",
    "typescript-tslint-plugin": "1.0.1",
    "url-loader": "4.1.1",
    "webpack": "^4",
    "webpack-cli": "^3",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "^3"
  }
}
